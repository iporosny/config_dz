(define debug_mode true)
(define api_version "v1")
(define cache_ttl 300)

app {
    name: "MyApplication"
    version: "1.0.0"

    settings {
        debug: debug_mode
        log_level: "info"
        api_version: api_version
    }

    modules {
        auth {
            enabled: true
            jwt_secret: "secret_key_123"
            expires_in: 3600
            algorithms: {"HS256", "RS256"}
        }

        cache {
            type: "redis"
            host: "localhost"
            port: 6379
            ttl: cache_ttl
            cluster: false
        }
    }
}
